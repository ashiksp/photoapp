<% provide(:title, 'New Photo') %>

<%= form_for(@album) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div class="field">
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_area :description, placeholder: "Type album description..." %>
</div>
	<%= f.submit  class: "btn btn-lg btn-primary" %>
<% end %>
